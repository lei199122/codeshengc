package com.sxt.demo;

public class Text {
	private static int num1 = 10;
	
	private static int num2 = 20;
	
	static {
		/*
		 * num1 = 100; num2 = 200;
		 */
		System.out.println("static block");
		System.out.println(num2);
	}
	
	public static void main(String[] args) {
		
		
			
			System.out.println(num1);
		
	}
}
