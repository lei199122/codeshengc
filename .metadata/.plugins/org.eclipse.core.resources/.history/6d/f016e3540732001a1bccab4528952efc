package com.sxt;

public class Hello {
	
		int A = 1;
		static int B = 2;
		static int C = 3;
		static int D = 4;
		int E = 5;
		int F = 6;
		static int G = 7;
		
		
	public static void main(String[] args) {
		System.out.println("你好");
		System.out.println("你在搞么事");
		add();
		
	}
	
	public static int add() {
		int a = B + C;
		int b = G + D;
		int c = a + b;
		return c ;
	}
}
